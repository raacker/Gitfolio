    <section class="bg-primary" id="biography" action="api/skillSets" method="post">
      <div class="container">
        <div class="row">
          <div class="col-lg-8 col-lg-offset-2 text-center">
            <h2 class="section-heading">Biography</h2>
            <hr class="light">
            <form class="biography-edit" action="api/userProfiles/edit" method="post">
              <label for="bio">Biography :</label>
              <textarea name="userBio" class="form-control" rows="5" id="bio"></textarea>
              <div id="linkedin">
                <h2 class="section-heading">Linked</h2>
                <hr class="light">
                <label for="url">LinkedIn</label>
                <input name="userLinkedInLink" id="url" type="string" class="form-control" size="60" placeholder="input LinkedIn link" required>
              </div>
              <input hidden="true" name="userID" value=<%= userID %>>
              <button type="submit" class="btn btn-default btn-xl sr-button" value="SUBMIT">Apply</button>
            </form>
            <form class="" action="api/skillSets" method="post">
              <label for="skillset">Skillset :</label>
              <hr class="light">
              <div id="skillset">
                <div class="checkbox">
                  <label><input type="checkbox" value="java">Java</label>
                  <label for="sel1">level :</label>
                  <select class="form-control" id="sel1">
                    <option>1</option>
                    <option>2</option>
                    <option>3</option>
                    <option>4</option>
                    <option>5</option>
                  </select>
                </div>
                <div class="checkbox">
                  <label><input type="checkbox" value="python">Python</label>
                  <label for="sel1">level :</label>
                  <select class="form-control" id="sel1">
                    <option>1</option>
                    <option>2</option>
                    <option>3</option>
                    <option>4</option>
                    <option>5</option>
                  </select>
                </div>
                <div class="checkbox">
                  <label><input type="checkbox" value="html">HTML</label>
                  <label for="sel1">level :</label>
                  <select class="form-control" id="sel1">
                    <option>1</option>
                    <option>2</option>
                    <option>3</option>
                    <option>4</option>
                    <option>5</option>
                  </select>
                </div>
                <div class="checkbox">
                  <label><input type="checkbox" value="javascript">Javascript</label>
                  <label for="sel1">level :</label>
                  <select class="form-control" id="sel1">
                    <option>1</option>
                    <option>2</option>
                    <option>3</option>
                    <option>4</option>
                    <option>5</option>
                  </select>
                </div>
                <div class="checkbox">
                  <label><input type="checkbox" value="c">C</label>
                  <label for="sel1">level :</label>
                  <select class="form-control" id="sel1">
                    <option>1</option>
                    <option>2</option>
                    <option>3</option>
                    <option>4</option>
                    <option>5</option>
                  </select>
                </div>
                <div class="checkbox">
                  <label><input type="checkbox" value="c++">C++</label>
                  <label for="sel1">level :</label>
                  <select class="form-control" id="sel1">
                    <option>1</option>
                    <option>2</option>
                    <option>3</option>
                    <option>4</option>
                    <option>5</option>
                  </select>
                </div>
              </div>
              <button type="submit" class="btn btn-default btn-xl sr-button" value="SUBMIT">Apply</button>
            </form>
            <form class="" action="api/organizations" method="post">
              <div id="organization">
                <h2 class="section-heading">Organiztion</h2>
                <hr class="light">
                <label for="url">Organization</label>
                <input name="name" id="url"s type="string" class="form-control" size="60" placeholder="input organization name" required>
                <button type="submit" class="btn btn-default btn-xl sr-button" value="SUBMIT">Apply</button>
              </div>
            </form>
          </div>
        </div>
      </div>
    </section>
    <section id="repository" action="api/repositories" method="post">
      <div class="container">
        <div class="row">
          <div class="col-lg-8 col-lg-offset-2 text-center">
            <h2 class="section-heading">Repository</h2>
            <hr class="light">
            <label for="url">URL :</label>
            <input id="url" type="string" class="form-control" size="60" placeholder="input repository link" required>
            <label for="description">Description :</label>
            <textarea class="form-control" rows="5" id="description"></textarea>
            <hr class="light">
            <label for="url">URL :</label>
            <input id="url" type="string" class="form-control" size="60" placeholder="input repository link" required>
            <label for="description">Description :</label>
            <textarea class="form-control" rows="5" id="description"></textarea>
            <hr class="light">
            <label for="url">URL :</label>
            <input id="url" type="string" class="form-control" size="60" placeholder="input repository link" required>
            <label for="description">Description :</label>
            <textarea class="form-control" rows="5" id="description"></textarea>
            <hr class="light">
            <label for="url">URL :</label>
            <input id="url" type="string" class="form-control" size="60" placeholder="input repository link" required>
            <label for="description">Description :</label>
            <textarea class="form-control" rows="5" id="description"></textarea>
            <button type="submit" class="btn btn-default btn-xl sr-button" value="SUBMIT">Apply</button>
          </div>
        </div>
      </div>
    </section>
  </body>
</html>
